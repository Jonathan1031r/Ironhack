<% if user_signed_in? %>
  <h2>Welcome <%= current_user.name %>! </h2>
  <%= link_to "Logout", destroy_user_session_path, method: :delete %>
<% else %>
  <h2> Nobody logged in </h2>
  <% end %>


<h2>List of Sandwiches:</h2>
<ul>
	<% @allSandwiches.each do |s| %>
		<li>
			<a href = "/sandwiches/<%= s.id %>">
				<%= s.name.capitalize %>
			</a>
		</li>
	<% end %>
</ul>